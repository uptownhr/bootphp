    if ($(this).prop('checked')) {
        $parentrow.find('input:not(.na)').prop('disabled', true);
        $parentrow.find('.icon-custom-equals').removeClass('show');
        $parentrow.find('.result').html('');
    } else {
        $parentrow.find('input:not(.na)').prop('disabled', false);
    }
});

